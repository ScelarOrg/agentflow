{
  "name": "@scelar/agentflow",
  "version": "1.0.2",
  "description": "100% Type-Safe AI Workflows",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc && tsc --emitDeclarationOnly --declaration --declarationMap",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "workflow",
    "streaming",
    "typescript",
    "ai-sdk",
    "llm",
    "agent",
    "react",
    "nextjs",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ScelarOrg/agentflow.git"
  },
  "bugs": {
    "url": "https://github.com/ScelarOrg/agentflow/issues"
  },
  "homepage": "https://github.com/ScelarOrg/agentflow#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@toon-format/toon": "^2.0.0",
    "nanoid": "^5.1.6"
  },
  "peerDependencies": {
    "ai": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "typescript": "^5.0.0",
    "zod": "^4.1.12"
  }
}
